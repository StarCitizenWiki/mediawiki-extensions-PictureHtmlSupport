{
	"name": "PictureHtmlSupport",
	"author": [
		"[https://www.mediawiki.org/wiki/User:Octfx Octfx]",
		"[https://www.mediawiki.org/wiki/User:Alistair3149 Alistair3149]"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:PictureHtmlSupport",
	"namemsg": "picturehtmlsupport",
	"descriptionmsg": "picturehtmlsupport-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.39.4",
		"platform": {
			"php": ">=8.0"
		}
	},
	"MessagesDirs": {
		"PictureHtmlSupport": [
			"i18n"
		]
	},
